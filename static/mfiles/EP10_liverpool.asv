clear; close all;
a = imread('EP10_liverpool.jpg');
quote = sprintf('We are the champions 2019/2020');
% f = figure;
% image(a)
h= image(a);
a = gca;
f = gcf;
a.DataAspectRatio = [1 1 1];
[width,height,~] = size(h.CData);

t = text(width/2,height-100,quote);
